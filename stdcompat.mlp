    let previous_index = ref (length s) in
    let accu = ref [] in
    for i = length s - 1 downto 0 do
      if unsafe_get s i = c then
        begin
          accu := sub s (i + 1) (!previous_index - i - 1) :: !accu;
          previous_index := i
        end
    done;
    sub s 0 !previous_index :: !accu
