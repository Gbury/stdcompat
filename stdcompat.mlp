#endif
#if OCAML_VERSION < (4,07,0)
    TO_SEQ('value' t, (key, 'value') internal)

    SEQ
#endif
  end

  module type SeededHashedType = sig
    type t

    val equal : t -> t -> bool

    val hash : int -> t -> int
